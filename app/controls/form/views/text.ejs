<div class="form-item" >
    <label for="<%= attrName %>"><%= label %></label>
    <input
        name="<%= attrName %>"
        type="text"
        data-type="<%= type %>"
        class="<% classes %>"
        value="<%= model.attr( attrName ) ? model.attr( attrName ) : '' %>"
        <%= (el) -> el.tooltip( tooltipOptions ) %>
    />
</div>
